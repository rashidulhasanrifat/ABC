	  <script>
	    var k;
	    var q = new Array (1,1,1,1,1);
	    var count = 0;
	    var house = new Array(5);
	          function Geeks() {
	              var input = document.getElementsByName('array[]');
	
	              for ( i = 0; i < input.length; i++) {
	                  var a = input[i];
	                  k = a.value ;
	                  house[i] = k;
	              }
	
	              for (j = 0; j < 5; j++){
	
	                    if(q[j]==house[j]){
	                      count = count+1;
	                    }
	
	
	                  //document.writeln(a.value);
	
	              }
	
	              if(count>2)
	              location.href = "file:///C:/Users/Musfiqur%20Alip/Desktop/petCare/patecareweb/canineparvovirus.html"
	              //var input2 = prompt('Roundworms');
	              else
	              location.href = "file:///C:/Users/Musfiqur%20Alip/Desktop/petCare/patecareweb/heartworms.html"
	
	              //var input2 = prompt('Salmonella');
	
	
	              //document.getElementById("par").innerHTML = k;
	              document.getElementById("po").innerHTML = "Output";
	          }
	
	
	
	
	
	
	  </script>
	
	
	
	
	
	
	
